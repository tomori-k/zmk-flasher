{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:allow-appdata-read",
    "fs:allow-temp-read",
    "fs:allow-appdata-write",
    "fs:allow-temp-write",
    "core:window:allow-destroy"
  ]
}
